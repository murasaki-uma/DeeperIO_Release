.wrap {
  background: inherit;
  animation: 7s spin ease-in-out infinite;
  -webkit-filter: contrast(25);
  filter: contrast(25);
  background: black;
  display none;
}

.hide
  animation: fadeout 2.5s ease-in-out .5s;

.hide02
  animation: fadeout 0s ease-in-out .5s;

.meta:before,
.meta:after {
  content: "";
  position: absolute;
  width:80px;
  height:80px;
  left:50%;
  top:50%;
  margin:-50px;
  background: rgba(180, 162, 200,.9);
  border-radius:50%;
  -webkit-filter: blur(30px);
  filter: blur(30px);
  border: 0px;
  animation: move 12s infinite alternate ease-in-out;

}
.meta:before {
  animation-duration: 19.3s;
  animation-delay: -3.3s;
}
.meta:nth-child(2):before {
  animation-duration: 14.7s;
  animation-delay: -2.7s;
}
.meta:nth-child(2):after {
  animation-duration: 8.7s;
  animation-delay: -5.32s;
}
@keyframes spin {
  to {
    /*     transform: rotate(360deg) */
  }
}
@keyframes move {
  0% {
    transform: translate(0%, 100%);
  }
  15% {
    transform: translate(-100%, 100%);
  }
  30% {
    transform: translate(80%, -60%);
  }
  40% {
    transform: translate(100%, 0%);
  }
  60% {
    transform: translate(40%, -80%);
  }
  80% {
    transform: translate(-80%, -20%);
  }
  100% {
    transform: translate(40%, 60%);
  }
}

@keyframes fadeout {
  0% { opacity: 1; }

  100% { opacity: 0.0; display: none!important; }
}


@keyframes fadein {
  0% { opacity: 0; }

  100% { opacity: 1.0; display: none; }
